<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	<appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
		<Encoding>UTF-8</Encoding>
		<encoder>
			<pattern>%d{MM/dd HH:mm:ss.SSS} [%-31thread] %-5level %msg [%logger{36}]%n
			</pattern>
		</encoder>
	</appender>

	<root level="INFO">
		<appender-ref ref="STDOUT" />
	</root>

	<!-- <logger name="service.WebHookService" level="DEBUG" /> <logger name="servlet" 
		level="DEBUG" /> -->
</configuration>